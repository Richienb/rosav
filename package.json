{
    "name": "@rosav/cli",
    "description": "A CLI that scans your files for matches to the VirusShare database.",
    "version": "0.0.0",
    "main": "index.js",
    "bin": {
        "rosav": "index.js"
    },
    "scripts": {
        "start": "node index.js",
        "build": "pkg --out-path dist ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ros-av/cli.git"
    },
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@octokit/rest": "^16.28.2",
        "bloomfilter": "^0.0.18",
        "bluebird": "^3.5.5",
        "chalk": "^2.4.2",
        "cli-progress": "^2.1.1",
        "cli-spinner": "^0.2.8",
        "commander": "^2.19.0",
        "dayjs": "^1.8.6",
        "esm": "^3.2.25",
        "fs-extra": "^8.1.0",
        "is-online": "^8.2.0",
        "line-by-line": "^0.1.6",
        "md5-file": "^4.0.0",
        "request": "^2.88.0",
        "request-progress": "^3.0.0",
        "request-promise": "^4.2.4",
        "temp-dir": "^2.0.0",
        "tiny-glob": "^0.2.6"
    },
    "devDependencies": {
        "pkg": "^4.3.7"
    },
    "pkg": {
        "targets": [
            "node10",
            "linux",
            "macos",
            "win",
            "x64"
        ]
    },
    "keywords": [
        "av",
        "antivirus",
        "viruslist"
    ],
    "bugs": {
        "url": "https://github.com/ros-av/cli/issues"
    },
    "homepage": "https://ros-av.github.io",
    "publishConfig": {
        "access": "public"
    }
}
